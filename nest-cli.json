{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/nestar-api/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/nestar-api/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/nestar-api",
  "projects": {
    "nestar": {
      "type": "application",
      "root": "apps/nestar-api",
      "entryFile": "main",
      "sourceRoot": "apps/nestar-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nestar-api/tsconfig.app.json"
      }
    },
    "nestar-betch": {
      "type": "application",
      "root": "apps/nestar-betch",
      "entryFile": "main",
      "sourceRoot": "apps/nestar-betch/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nestar-betch/tsconfig.app.json"
      }
    }
  }
}
